   "metadata": {},
   "source": [
    "Dimensionality reduction using PCA"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 49,
   "id": "cf4ce819",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Number of components to retain 95.0% of variance: 24\n"
     ]
    }
   ],
   "source": [
    "# Perform PCA\n",
    "pca = PCA()\n",
    "pca.fit(scaled_features)\n",
    "\n",
    "# Calculate cumulative sum of explained variance ratio\n",
    "cumulative_variance = np.cumsum(pca.explained_variance_ratio_)\n",
    "\n",
    "# Determine the number of components that explain a certain percentage of variance\n",
    "desired_variance = 0.95  # Example: retain 95% of variance\n",
    "n_components = np.argmax(cumulative_variance >= desired_variance) + 1\n",
    "\n",
    "print(\"Number of components to retain {}% of variance: {}\".format(desired_variance * 100, n_components))\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "02acb68e",
   "metadata": {},
   "outputs": [],
   "source": [
    "from sklearn.decomposition import IncrementalPCA\n",
    "\n",
    "# Assuming X is your large dataset with shape (n_samples, n_features)\n",
    "\n",
    "# Define the batch size for IPCA\n",
    "batch_size = 1000\n",
    "\n",
    "# Initialize IPCA with the desired number of components\n",
    "n_components = 24\n",
    "ipca = IncrementalPCA(n_components=n_components, batch_size=batch_size)\n",
    "\n",
    "# Iterate over the dataset in batches and fit IPCA incrementally\n",
    "for i in range(0, len(X), batch_size):\n",
    "    X_batch = X[i:i+batch_size]\n",
    "    ipca.partial_fit(X_batch)\n",
    "\n",
    "# Transform the entire dataset using IPCA\n",
    "X_ipca = ipca.transform(X)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "c9f8375a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Perform PCA\n",
    "pca = PCA(n_components=24)\n",
    "X_pca = pca.fit_transform(scaled_features)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "243ebb0a",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Create a DataFrame from the transformed array\n",
    "X_pca_df = pd.DataFrame(data=X_pca, columns=[f\"PC{i+1}\" for i in range(X_pca.shape[1])])\n",
    "\n",
    "# X_pca_df now contains the reduced-dimensional data with n_components as columns"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
